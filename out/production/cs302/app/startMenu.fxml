<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="startPane" prefHeight="400.0" prefWidth="600.0" stylesheets="@app.css"
            xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="app.controllers.StartMenuController">
    <children>
        <Button fx:id="chooseFile" layoutX="76.0" layoutY="179.0" mnemonicParsing="false" onAction="#chooseFile"
                stylesheets="@app.css" text="請選擇文件" textFill="LIME"/>
        <Button fx:id="start" layoutX="375.0" layoutY="179.0" mnemonicParsing="false" onAction="#startPlay"
                stylesheets="@app.css" text="開始" textFill="LIME"/>
        <Label fx:id="fileName" layoutX="192.0" layoutY="187.0" prefHeight="19.0" prefWidth="169.0" textFill="LIME"/>
    </children>
</AnchorPane>
